@model EmployeeViewModel


@{
    ViewData["Title"] = "Create";

}

<h1>Create Employee Page</h1>
<br/>
<br/>
<form asp-action="Create" enctype="multipart/form-data">

    <partial name="EmployeePartialViews/EditCreateEmployeePartialView" model="Model" />

    <input type="submit" value="Create" class="btn btn-primary" />
    <a asp-controller="Employee" asp-action="Index" class="btn btn-light">Back To List</a>
</form>


@section ClientValidation{

    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>


}